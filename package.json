{
  "private": true,
  "description": "A multichain wallet connect library",
  "repository": "git@github.com:civicteam/civic-multichain-connect-react.git",
  "author": "civic.com",
  "license": "MIT",
  "files": [
    "package.json",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "nuke": "shx rm -rf packages/*/*/node_modules node_modules pnpm-lock.yaml || true",
    "reinstall": "pnpm run nuke && pnpm install",
    "build:core": "pnpm --filter \"./packages/core\" run build",
    "build:adapters": "pnpm --filter \"./packages/solana-wallet-adapter\" --filter \"./packages/rainbowkit-wallet-adapter\" run build",
    "build:all": "pnpm run build:core && pnpm run build:adapters",
    "build:clean": "pnpm run clean && pnpm run build:all",
    "publish:beta": "pnpm publish -r --tag beta",
    "publish:release": "pnpm publish -r"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.15",
    "@types/mocha": "^9.1.1",
    "@types/ramda": "^0.28.25",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint_d": "^12.2.1",
    "generate-changelog": "^1.8.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.15.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
