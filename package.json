{
  "name": "@civic/multichain-connect-react",
  "version": "0.0.1-beta.2",
  "description": "A multichain wallet connect library",
  "repository": "git@github.com:civicteam/civic-multichain-connect-react.git",
  "author": "civic.com",
  "license": "MIT",
  "files": [
    "package.json",
    "dist"
  ],
  "scripts": {
    "test": "yarn test:unit",
    "test:unit": "STAGE=${STAGE:-local} FORCE_COLOR=1 LOG_LEVEL=error LOCAL=true  ts-mocha -p tsconfig.mocha.json --timeout 20000 --paths 'test/unit/**/*.ts'",
    "test:mocha": "STAGE=${STAGE:-local} FORCE_COLOR=1 LOG_LEVEL=error LOCAL=true nyc --reporter=text --reporter=html mocha",
    "test:watch": "ts-mocha -p tsconfig.mocha.json 'test/**/*.ts' -w --watch-files '**/**/*.ts'",
    "build": "tsc -b tsconfig.build.json && tsc -b tsconfig.umd.json",
    "lint": "eslint -c .eslintrc.js '{src,test}/**/*.ts'",
    "lint:fix": "eslint -c .eslintrc.js --fix '{src,test}/**/*.ts'"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@civic/civic-chain-icons": "0.0.5",
    "@civic/civic-eth-provider": "0.0.8",
    "@headlessui/react": "^1.7.14",
    "@rainbow-me/rainbowkit": "^0.12.14",
    "@solana/wallet-adapter-base": "^0.9.22",
    "@solana/wallet-adapter-react": "^0.15.32",
    "@solana/wallet-adapter-react-ui": "^0.9.31",
    "@solana/wallet-adapter-wallets": "^0.19.16",
    "@solana/wallet-standard-wallet-adapter-react": "^1.0.2",
    "@solana/web3.js": "^1.76.0",
    "@types/react": "^17.0.0",
    "@wallet-standard/app": "^1.0.1",
    "@wallet-standard/core": "^1.0.3",
    "@wallet-standard/react-core": "^0.1.4",
    "@wallet-standard/wallet": "^1.0.1",
    "ethers": "^5",
    "ramda": "^0.28.0",
    "styled-components": "^6.0.0-rc.2",
    "wagmi": "^0.12.13"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/expect": "^24.3.0",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.1.0",
    "@types/ramda": "^0.28.12",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint_d": "^12.0.0",
    "husky": "^8.0.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.5.2",
    "typescript": "^4.1.2"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts"
}
